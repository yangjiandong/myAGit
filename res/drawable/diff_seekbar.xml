<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip"/>
            <gradient
                    android:startColor="#ffed0700"
                    android:centerColor="#ff5a5d5a"
                    android:centerX="0.5"
                    android:endColor="#ff19b800"
                    android:angle="0"
                    />
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip"/>
                <gradient
                        android:startColor="#ffd50600"
                        android:centerColor="#ff5a5d5a"
                        android:centerX="0.5"
                        android:endColor="#ff1ed600"
                        android:angle="0"
                        />
            </shape>
        </clip>
    </item>

</layer-list>
